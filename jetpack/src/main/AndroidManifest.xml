<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jetpack">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:name=".MyLifecycle.ProcessLifecycleOwnerApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Study">
        <activity
            android:name=".MyliveData.MyViewModelActivity2"
            android:exported="false" />
        <activity
            android:name=".MyliveData.MyLivaDataActivity"
            android:exported="false" />
        <activity
            android:name=".MyViewModel.MyViewModelActivity"
            android:exported="false" />
        <activity
            android:name=".MyLifecycle.LifecycleServiceActivity"
            android:exported="false" />
        <activity
            android:name=".MyLifecycle.LifeCycleActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".MyLifecycle.MyService" />
    </application>

</manifest>